{
  "$schema": "https://raw.githubusercontent.com/DanielGavin/ols/master/misc/odinfmt.schema.json",
  "tabs": true,
  "tabs_width": 2,
  "character_width": 80,
  "indent_cases": true,
  "convert_do": true,
  "newline_style": "LF",
  "newline_limit": 2,
  "brace_style": "_1TBS",
  "sort_imports": true,
  "inline_single_stmt_case": false,
  "exp_multiline_composite_literals": true
}
